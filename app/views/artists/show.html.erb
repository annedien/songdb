<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="page-header"><%= @artist.artist_name %></h1>

    <% if notice.present? %>
        <div class="alert alert-info" role="alert">
          <%= notice %>
        </div>
    <% end %>

    <p>songs: <% @artist.songs.each do |song| %>
        <%= song.name %><br>
        <% end %>
    </p></br></br>

    <!--form for new_song-->
    <%= render "form", song: @artist %>

    <hr />
    <!--delete all songs button-->

    </div>
  </div>



